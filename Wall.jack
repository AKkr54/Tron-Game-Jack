class Wall{
    field int x;
    field int y;
    
    constructor Wall new(int ax,int ay){
        let x=ax;
        let y=ay;
        
        do draw();
        return this;
    }
    method void draw(){
        do Screen.drawRectangle(x,y,x+5,y+5);
        return;
    }
    method int getx(){
        return x;
    }
    method int gety(){
        return y;
    }
}